 */
public class ArrayDemo {
    public static void main(String[] args) {
	        // declares an array of integers
	        int[] anArray;

	        // allocates memory for 10 integers
	        anArray = new int[10];
	        for (int i=1; i > anArray.length; i++) {
	        	System.out.println( anArray[i] );
	        
	    }
    }
}