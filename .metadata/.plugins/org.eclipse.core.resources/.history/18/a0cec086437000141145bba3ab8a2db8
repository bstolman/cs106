import java.io.Console;


public class Recursionnn {
	static int i = 0;
	public static long main(int j)
	{
	    // This is the base case
	    if(i == 0)
	    {
	         return 1;
	    }
	    else
	    {
	        // This reduces the problem to something closer to the base case
	        return i * main(i - 1);
	    }
	}

}