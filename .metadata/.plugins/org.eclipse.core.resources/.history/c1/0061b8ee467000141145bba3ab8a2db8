import java.io.Console;

public class Recursionnn {
	 
	    public static void main(String[] args) {  
	        poop(10);      
	    }  
	      // print 1 to n  
	    private static void poop(int n) {  
	        if (n > 1) {  
	            poop(n-1);  
	        }  
	        System.out.println(n);  
	    }
}

		