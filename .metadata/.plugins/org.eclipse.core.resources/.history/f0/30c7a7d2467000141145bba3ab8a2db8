import java.io.Console;

public class Recursionnn {
	 
	    public static void main(String[] args) {  
	        show(10);      
	    }  
	      
	    private static void show(int i) {
			// TODO Auto-generated method stub
			
		}

		// print 1 to n  
	    private static void display(int n) {  
	        if (n > 1) {  
	            show(n-1);  
	        }  
	        System.out.println(n);  
	    }
}

		