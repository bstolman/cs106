public class Track {
	String number;
	String time;
	String artist;
	String song;

	public Track(String number, String time, String artist, String song) {
		this.number = number;
		this.time = time;
		this.artist = artist;
		this.song = song;
	}

	public String toString() {
		return number + "\t" + time + "\t" + artist + "\t"
				+ song + "\t";
	}
}
